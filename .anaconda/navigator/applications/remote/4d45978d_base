ai_navigator:
  app_type: installable
  description: Access various large language models (LLMs) curated by Anaconda, and
    start leveraging secure local AI today.
  detector:
    args:
    - args:
      - args:
        - ai_navigator_path
        type: check_configured_roots
      - args:
        - AI-Navigator.app
        type: check_known_osx_roots
      - args:
        - - Contents
          - MacOS
          - AI-Navigator
        type: append_executable
      type: osx_only
    - args:
      - args:
        - ai_navigator_path
        type: check_configured_roots
      - args:
        - - '{windows/local_app_data}'
          - Programs
          - ai-navigator
        type: check_known_roots
      - args:
        - - AI-Navigator.exe
        type: append_executable
      type: windows_only
    type: group
  display_name: Anaconda AI Navigator
  extra_arguments: null
  image_path: /Users/pastrysweet/.anaconda/navigator/applications/remote/a7d973ac_ai-navigator.f364e.svg
  install:
    fallback_url: https://anaconda.cloud/api/navigator/v1/applications/open/ai_navigator
    func: ::install_dmg
    installer_url: https://autoupdate.ai.anaconda.com/latest-AI-Navigator-arm64.dmg?utm=anaconda-navigator
  is_available: true
  rank: 4
