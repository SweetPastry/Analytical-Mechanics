{
  "deps": [],
  "macros": [
    {
      "name": "newfloat",
      "arg": {
        "format": "{}{}{}",
        "snippet": "newfloat{${1:float type%envname}}{${2:placement}}{${3:ext}}"
      }
    },
    {
      "name": "newfloat",
      "arg": {
