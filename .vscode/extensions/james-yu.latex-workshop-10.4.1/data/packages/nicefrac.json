{
  "deps": [
    {
      "name": "ifthen"
    }
  ],
  "macros": [
    {
      "name": "nicefrac",
      "arg": {
        "format": "[]{}{}",
        "snippet": "nicefrac[${3:fontcmd%:translatable}]{${1:num%:translatable}}{${2:den%:translatable}}"
      }
    },
    {
      "name": "nicefrac",
      "arg": {
        "format": "{}{}",
        "snippet": "nicefrac{${1:num%:translatable}}{${2:den%:translatable}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/nicefrac#c": [
      "nice",
      "ugly"
    ]
  },
  "args": [
    "\\usepackage/nicefrac#c"
  ]
}