    {
      "name": "captionblock",
      "arg": {
        "format": "{}",
        "snippet": "{${1:width}}"
      }
    },
    {
      "name": "captionblock",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:position}]{${1:width}}"
