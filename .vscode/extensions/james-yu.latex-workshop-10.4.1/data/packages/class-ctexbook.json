{
  "deps": [
    {
      "name": "ctex"
    },
    {
      "name": "class-book"
    }
  ],
  "macros": [
    {
      "name": "partmark",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "partmark{${1:code}}"
      }
    },
    {
      "name": "CTEXnumberline",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "CTEXnumberline{${1:number}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\documentclass/ctexbook#c": [
      "GBK",
      "UTF8",
      "zhmap=${1|true,false,zhmCJK|}",
      "zihao=${1|-4,5,false|}",
      "heading",
      "sub3section",
      "sub4section",
      "scheme=${1|chinese,plain|}",
      "linespread=${1:number}",
      "fontset=${1|adobe,fandol,founder,mac,macnew,macold,ubuntu,windows,none|}",
      "punct=${1|quanjiao,banjiao,kaiming,CCT,plain|}",
      "space=${1|true,false,auto|}",
      "autoindent=",
      "a4paper",
      "a5paper",
      "b5paper",
      "letterpaper",
      "legalpaper",
      "executivepaper",
      "landscape",
      "10pt",
      "11pt",
      "12pt",
      "oneside",
      "twoside",
      "draft",
      "final",
      "titlepage",
      "notitlepage",
      "openright",
      "openany",
      "onecolumn",
      "twocolumn",
      "leqno",
      "fleqn",
      "openbib"
    ]
  },
  "args": [
    "\\documentclass/ctexbook#c"
  ]
}