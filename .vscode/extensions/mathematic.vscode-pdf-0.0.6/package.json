{
	"name": "vscode-pdf",
	"displayName": "PDF Viewer",
	"description": "Portable document format (PDF) viewer for Visual Studio Code.",
	"version": "0.0.6",
	"publisher": "mathematic",
	"engines": {
		"vscode": "^1.58.1"
	},
	"icon": "icon.png",
	"license": "Apache-2.0",
	"keywords": [
		"pdf",
		"viewer"
	],
	"categories": [
		"Visualization",
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:pdf.view"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "pdf.view",
				"displayName": "PDF View",
				"selector": [
					{
						"filenamePattern": "*.pdf"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "yarn run compile",
		"test-watch": "yarn run watch"
	},
	"devDependencies": {
		"@types/node": "16.x",
		"@types/vscode": "^1.58.1",
		"ts-loader": "^9.2.4",
		"typescript": "^4.3.5",
		"webpack": "^5.47.1",
		"webpack-cli": "^4.7.2"
	},
	"dependencies": {},
